generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum OS {
  ios
  android
}

model Device {
  token      String   @id @db.VarChar(256)
  os         OS
  created_at DateTime @db.Timestamp(3)
  updated_at DateTime @db.Timestamp(3)
}
