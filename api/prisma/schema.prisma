generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Device {
  token      String   @id @db.VarChar(256)
  os         String   @db.VarChar(7)
  created_at DateTime @db.Timestamp(3)
  updated_at DateTime @db.Timestamp(3)
}
